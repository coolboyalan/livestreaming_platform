<style>
    .thumbnail {
    position: relative;
    overflow: hidden;
    border-radius: 50%; /* Make the image circular */
    width: 200px; /* Set your desired width */
    height: 200px; /* Set your desired height */
}

.thumbnail img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.thumbnail::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0); /* Initially transparent */
    transition: background-color 0.3s ease;
}

.thumbnail:hover::before {
    background-color: rgba(0, 0, 0, 0.5); /* Black with 50% opacity */
}

.content {
    position: absolute;
    top: 50%;
    left: 40%;
    transform: translate(-50%, -50%);
    text-align: center;
    color: #fff; /* Set text color to white */
    z-index: 1; /* Ensure content is above the overlay */
    opacity: 0; /* Initially hide the content */
    transition: opacity 0.3s ease;
}



.rbt-cat-box:hover .content {
    opacity: 1; /* Show the content on hover */
}

.title {
    font-size: 18px;
    margin-bottom: 10px;
    color: #ffffff;
}

.read-more-btn {
    display: inline-block;
    background-color: #FFBC09;
    color: #fff;
    padding: 8px 16px;
    border-radius: 20px;
    cursor: pointer;
}

.rbt-btn-link {
    text-decoration: none;
    color: #fff;
}

.rbt-btn-link:hover {
    text-decoration: none;
    color: #fff;
}


</style>



<!-- START SECTION REGISTER -->
<section class="pb-0 background_bg bg_orange" data-img-src="/assets/images/pattern_bg.png">
	<div class="container">
    	<div class="row align-items">
            
            <div class="col-lg-6 col-md-7">
            	<img src="/assets/images/man_img.png" alt="">
                <div class="large_divider"></div>
            </div>
            <div class="col-lg-6 col-md-7">
            	<div class="register_form radius_all_10 text_white padding_eight_all animation" data-animation="fadeInLeft" data-animation-delay="0.02s">
                    <div class="heading_s1 heading_light">
                        <h2>Are You a <span class="text_light_navy">Content Viewer</span></h2>
                    </div>
                    <p>If you are going to use a passage of Lorem Ipsum, you need to be sure there isn't anything hidden in the middle of text</p>
                    <form id="myForm" name="enq" method="post" class="pt-md-2 form_transparent" >
                        <div class="row">
                            <div class="form-group col-sm-12">
                                <label for="email">Enter Your Email Address</label>
                                <input id="email" required class="form-control" name="email" type="email">
                            </div>
                            <div class="form-group col-sm-12">
                                <label for="username">Enter Your Username</label>
                                <input required class="form-control" name="username" type="text">
                            </div>
                            <div class="form-group col-sm-12">
                                <label for="password">Choose a Password</label>
                                <input required class="form-control" name="password" type="password">
                            </div>
                            <div class="form-group col-sm-12">
                                <label for="confirm-password">Re-enter Your Password</label>
                                <input required class="form-control" name="confirm-password" type="password">
                            </div>
                            <div class="form-group col-sm-12 d-flex" style="padding: 20px 20px;">
                                <label for="18 year">I'm Over 18 Years Old</label>
                                <div class="form-check ml-4">
                                    <input class="form-check-input" type="checkbox" value="true" name="isAdult" id="defaultCheck1">
                                    <label class="form-check-label" for="isAdult">Yes</label>
                                </div>
                                <div class="form-check ml-4">
                                    <input class="form-check-input" type="checkbox" value="false" name="isAdult" id="defaultCheck2">
                                    <label class="form-check-label" for="notAdult">No</label>
                                </div>
                            </div>
                            <input type="text" name="category" value="viewer" style="display: none;">
                            <div class="col-md-12">
                                <button type="submit" id="submitButton" title="Submit Your Message!" class="btn btn-default" name="submit" value="Submit" style="background-color: #554DA7;">Register Now</button>
                            </div>
                            <div class="col-sm-12">
                                <div id="alert-msg" class="alert-msg text-center"></div>
                            </div>
                        </div>
                    </form>
                    
                    <script>
                        // Wait for the DOM content to be fully loaded
                        document.addEventListener("DOMContentLoaded", function() {
                            document.getElementById('myForm').addEventListener('submit', function(event) {
                                const form = this;
                                var formData = new FormData(this);
                    
                                var email = formData.get('email').trim();
                                var username = formData.get('username').trim();
                                var password = formData.get('password').trim();
                                var confirmPassword = formData.get('confirm-password').trim();
                                var emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                                var isAdult = formData.get('isAdult');
                    
                                if (!emailPattern.test(email)) {
                                    document.getElementById('alert-msg').textContent = 'Please enter a valid email address.';
                                    event.preventDefault();
                                    return;
                                }
                    
                                if (username === '') {
                                    document.getElementById('alert-msg').textContent = 'Please enter your username.';
                                    event.preventDefault();
                                    return;
                                }
                    
                                if (password === '') {
                                    document.getElementById('alert-msg').textContent = 'Please choose a password.';
                                    event.preventDefault();
                                    return;
                                }
                    
                                if (confirmPassword === '') {
                                    document.getElementById('alert-msg').textContent = 'Please re-enter your password.';
                                    event.preventDefault();
                                    return;
                                }
                    
                                if (password !== confirmPassword) {
                                    document.getElementById('alert-msg').textContent = 'Passwords do not match.';
                                    event.preventDefault();
                                    return;
                                }
                                if (!isAdult) {
                                    document.getElementById('alert-msg').textContent = 'Please confirm your age';
                                    event.preventDefault();
                                    return;
                                }
                                document.getElementById('alert-msg').textContent = '';
                        });
                    });
                    </script>
                            
                            
                </div>
                
            </div>
        </div>
    </div>
</section>


  


