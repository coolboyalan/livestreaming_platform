    <script>
      function validateForm() {
        var firstName = document.getElementById("firstName").value;
        var lastName = document.getElementById("lastName").value;
        var sex = document.getElementById("sex").value;
        var nativeLanguage = document.getElementById("nativeLanguage").value;
        var contentCategory = document.getElementById("contentCategory").value;
        var dob = document.getElementById("dob").value;
        var governmentIdNo = document.getElementById("governmentIdNo").value;
        var country = document.getElementById("country").value;
        var city = document.getElementById("city").value;
        var state = document.getElementById("state").value;
        var postalCode = document.getElementById("postalCode").value;
        var address = document.getElementById("address").value;
        var companyName = document.getElementById("companyName").value;
        var taxNo = document.getElementById("taxNo").value;
        var businessRegistrationNo = document.getElementById(
          "businessRegistrationNo"
        ).value;
        var alert = document.getElementById("alert-msg");

        if (firstName.trim() === "") {
          alert.innerText = "First Name cannot be empty.";
          event.preventDefault;
        }
        if (lastName.trim() === "") {
          alert.innerText = "Last Name cannot be empty.";
          event.preventDefault;
        }
        const gender= ["Male","Female","Others"]
        if (sex.trim() === "" || !gender.includes(sex)) {
          alert.innerText = "Please select Sex.";
          event.preventDefault;
        }
        if (nativeLanguage.trim() === "") {
          alert.innerText = "Please select Native Language.";
          event.preventDefault;
        }
        if (contentCategory.trim() === "") {
          alert.innerText = "Please select Content Category.";
          event.preventDefault;
        }
        if (dob.trim() === "") {
          alert.innerText = "Date Of Birth cannot be empty.";
          event.preventDefault;
        }
        if (governmentIdNo.trim() === "") {
          alert.innerText = "Number of Your ID or Passport cannot be empty.";
          event.preventDefault;
        }
        if (country.trim() === "") {
          alert.innerText = "Please select Country.";
          event.preventDefault;
        }
        if (city.trim() === "") {
          alert.innerText = "City cannot be empty.";
          event.preventDefault;
        }
        if (state.trim() === "") {
          alert.innerText = "State cannot be empty.";
          event.preventDefault;
        }
        if (postalCode.trim() === "") {
          alert.innerText = "Postal code cannot be empty.";
          event.preventDefault;
        }
        if (address.trim() === "") {
          alert.innerText = "Address cannot be empty.";
          event.preventDefault;
        }
        if (companyName.trim() === "") {
          alert.innerText = "Company Name cannot be empty.";
          event.preventDefault;
        }
        if (taxNo.trim() === "") {
          alert.innerText = "VAT number or Tax ID cannot be empty.";
          event.preventDefault;
        }
        if (businessRegistrationNo.trim() === "") {
          alert.innerText = "Business Registration cannot be empty.";
          event.preventDefault;
        }
        alert.innerText = "";
      }
    </script>

